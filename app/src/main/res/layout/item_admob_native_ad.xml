<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/native_ad_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.maxrave.simpmusic.ads.nativead.AppNativeAdView
        android:id="@+id/admob_template_small"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:native_ad_type="@layout/admob_native_ad_small" />

    <com.maxrave.simpmusic.ads.nativead.AppNativeAdView
        android:id="@+id/admob_template_medium"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:native_ad_type="@layout/admob_native_ad_medium" />

</LinearLayout>